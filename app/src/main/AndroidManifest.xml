<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alberto.tesisjunta">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/baseball"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LOGIN.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LOGIN.RegisterActivity"
            android:label="@string/app_name"/>
        <activity
            android:name=".MulticamaraActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".VideoGrande"
            android:label="@string/app_name"
            android:parentActivityName=".MulticamaraActivity" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MulticamaraActivity" />
        />
        <activity
            android:name=".Informacion"
            android:label="@string/app_name"
            android:parentActivityName=".MulticamaraActivity" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MulticamaraActivity" />
        />
        <activity
            android:name=".Menus"
            android:label="@string/app_name"
            android:parentActivityName=".MulticamaraActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MulticamaraActivity" />
            >
        </activity>
        <activity
            android:name=".DataWeather.Weather"
            android:label="@string/app_name"
            android:parentActivityName=".MulticamaraActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MulticamaraActivity" />
        </activity>
        <activity android:name=".DataStandings.National"
            android:label="@string/app_name"
            android:parentActivityName=".MulticamaraActivity"/>
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MulticamaraActivity" />
        <activity android:name=".DataStandings.American"
            android:label="@string/app_name"
            android:parentActivityName=".MulticamaraActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MulticamaraActivity"/>



        </activity>
    </application>

</manifest>